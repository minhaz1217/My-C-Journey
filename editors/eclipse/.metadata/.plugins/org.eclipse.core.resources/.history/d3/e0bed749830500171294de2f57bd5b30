#include<iostream>
using namespace std;
int main(){
    int a,m,f,i,j,temp;
    cin >> a;
    while(a--){
        cin >> m>>f;
        for(i=1;i<=f;i++){
            for(j=1;j<=m;j++){
                temp=j;
                while(temp--){
                    cout << j;
                }
                cout << endl;
            }
            for(j=m-1;j>=1;j--){
                temp=j;
                while(temp--){
                    cout << j;
                }
                cout << endl;
            }
            if(a-1 != 0){
                cout << endl;

            }
        }
    }
	return 0;
}
