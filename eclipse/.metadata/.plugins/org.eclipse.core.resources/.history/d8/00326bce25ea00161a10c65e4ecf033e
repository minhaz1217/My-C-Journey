#include<bits/stdc++.h>
using namespace std;
#define check(a) cout<<a<< endl;
int main(){
    long long tc,a,b,ans,i;
    cin >> tc;
    while(tc--){
        cin >> a >> b;
        a -= 2;
        b -= 2;
        for(i=1;i*3 <= a;i++){}
        a= i;
        for(i=1;i*3 <= b;i++){}
        b = i;
        cout << a*b << endl;
    }
	return 0;
}
