#include<bits/stdc++.h>
using namespace std;
#define check(a) cout<<a<< endl;
int main(){
    long long int tc,a,b,ans;
    cin >> tc;
    while(tc--){
        cin >> a >> b;
        a -= 2;
        b-=2;

        ans = (int)ceil(a/3) * (int)ceil(b/3);
        cout << ans << endl;
    }
	return 0;
}
